{
  "name": "cftools-sdk",
  "version": "1.0.2",
  "description": "An easy to use JavaScript implementation to talk with the CFTools Cloud API from your JavaScript project.",
  "repository": "github:FlorianSW/cftools-sdk",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rm -rf lib && tsc",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "dotenv": "^9.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "got": "^11.8.2"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "cftools",
    "sdk"
  ],
  "author": "Florian Schmidt",
  "license": "MIT"
}
